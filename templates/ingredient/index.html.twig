    $('.slide').eq(index).addClass('active-slide');

    $('.slider-dot').removeClass('active-dot');
    $('.slider-dot').eq(index).addClass('active-dot');
  }

  function nextSlide() {
    currentSlideIndex = (currentSlideIndex + 1) % $('.slide').length;
    showSlide(currentSlideIndex);
  }

  function prevSlide() {
    currentSlideIndex = (currentSlideIndex - 1 + $('.slide').length) % $('.slide').length;
    showSlide(currentSlideIndex);
  }

  function currentSlide(index) {
    currentSlideIndex = index;
    showSlide(currentSlideIndex);
  }
</script>


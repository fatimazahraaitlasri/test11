     justify-content: center;
     align-items:center;

       
      position: relative;
    }

    .slide {
      display: none;
      text-align: center;
  
    }

    .active-slide {
      display: block;
    }

    .slider-buttons {
      
   

    }

 

    .slider-dots {
      position: absolute;
      top: 150px;
      width: 100%;
      display: flex;
      justify-content: center;
    }

